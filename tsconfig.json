{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "sourceMap": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es6"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
