{
  "name": "code-lol",
  "displayName": "CodeLol",
  "description": "Lolcat for VSCode",
  "version": "1.0.0",
  "publisher": "EnsignPayton",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Formatters"
  ],
  "main": "./dist/main",
  "activationEvents": [
    "onCommand:lol.cat"
  ],
  "contributes": {
    "commands": [
      {
        "command": "lol.cat",
        "title": "lolcat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode'bin/test"
  },
  "devDependencies": {
    "@types/node": "^10.12.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "vscode": "^1.1.21"
  }
}
